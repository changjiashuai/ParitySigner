<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/log"
    app:startDestination="@+id/logScreen">

    <fragment
        android:id="@+id/logScreen"
        android:name="com.changjiashuai.paritysigner.ui.log.LogFragment"
        android:label="Log"
        tools:layout="@layout/fragment_log">
        <action
            android:id="@+id/action_log_to_details"
            app:destination="@id/logDetails" />
    </fragment>
    <fragment
        android:id="@+id/logDetails"
        android:name="com.changjiashuai.paritysigner.ui.log.LogDetailsFragment"
        android:label="Log Details"
        tools:layout="@layout/fragment_log_details">

        <argument
            android:name="order"
            app:argType="string" />

    </fragment>
</navigation>
